{
  "name": "synxty-components",
  "displayName": "Synxty Components",
  "description": "Create your React components right from the explorer!",
  "version": "1.0.0",
  "publisher": "synxty",
  "repository": {
    "type": "git",
    "url": "https://github.com/synxty/synxty-components"
  },
  "license": "MIT",
  "icon": ".github/assets/icon.png",
  "keywords": [
    "react",
    "next",
    "styled-components",
    "synxty",
    "create-synxty-app",
    "create-next-app",
    "create-react-app",
    "cra",
    "typescript",
    "tsx",
    "jest",
    "ts",
    "components",
    "styled"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "galleryBanner": {
    "color": "#222222",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "activationEvents": [
    "onCommand:extension.synxty-react-components",
    "onCommand:extension.synxty-next-components"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.synxty-react-components",
        "title": "Create a React Component"
      },
      {
        "command": "extension.synxty-next-components",
        "title": "Create a Next Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.synxty-react-components",
          "group": "1_modification",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "extension.synxty-next-components",
          "group": "1_modification",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "build": "tsc -p ./",
    "dev": "tsc -w -p ./",
    "vscode:prepublish": "yarn build",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^14.10.0",
    "@types/vscode": "^1.49.0",
    "typescript": "^4.0.2"
  }
}